{
    "main":{
        "logFile":"/var/log/mplane/reasoner.log",
        "version":"1.0",
        "description": "mPlane REASONER",
        "prompt_separator":"#",
        "minimum_columns_required":150,
        "process_name": "mPlane Reasoner"
    },
    "supervisor":{
        "listenPort":2427,
        "hostName":"mplane.org"
    },
    "registry":{
        "file":"./registry.json"
    },
    "ssl":{
        "key": "./ssl/official/reasoner/reasoner-plaintext.key.pem"
        ,"cert": "./ssl/official/reasoner/reasoner.crt.pem"
        ,"ca": [ "./ssl/official/root-ca.pem" , "./ssl/official/signing-ca.pem" ]
        ,"requestCert" : true
    }
}